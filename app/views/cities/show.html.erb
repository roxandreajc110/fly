<div class="container">
  <article class="row city">
    <ul>
      <li class="col-md-3"><strong><%= @city.name %></strong></li>
      <li class="col-md-3"><%= @city.country_name %></li>
      <li class="col-md-3"><%= @city.time_zone %></li>
      <li class="col-md-3"><a href="<%= city_path(@city.id) %>" data-method="delete" class="btn btn-danger"> Delete </a></li>
    </ul>
  </article>
</div>
